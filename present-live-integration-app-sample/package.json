  "resolutions": {
    "@azure/msal-browser": "3.26.1",
    "uuid": "10.0.0"
  },
  "babel": {
    "presets": [],
    "plugins": [
      [
        "@babel/plugin-transform-nullish-coalescing-operator"
      ],
      [
        "@babel/plugin-transform-optional-chaining"
      ]
    ]
  },
  "browser": {
    "crypto": false,
    "stream": false
  }
